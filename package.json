{
    "name": "rentalx",
    "version": "1.0.0",
    "main": "index.js",
    "license": "MIT",
    "scripts": {
        "build": "babel src --extensions \".js,.ts\" --out-dir dist --copy-files",
        "typeorm": "set NODE_ENV=dev&& ts-node-dev -r tsconfig-paths/register ./node_modules/typeorm/cli",
        "dev": "ts-node-dev -r tsconfig-paths/register --poll --transpile-only --inspect --ignore-watch node_modules --respawn src/shared/infra/http/server.ts",
        "migration:create": "yarn typeorm migration:create src/shared/infra/typeorm/migrations/%name%",
        "migration:generate": "yarn typeorm -d ./src/shared/infra/typeorm/data-source.ts migration:generate ./src/shared/infra/typeorm/migrations/generate/",
        "migration:run-dev": "yarn typeorm -d ./src/shared/infra/typeorm/data-source.ts migration:run",
        "migration:run-prod": "node ./node_modules/typeorm/cli -d ./dist/shared/infra/typeorm/data-source.js migration:run",
        "migration:revert": "yarn typeorm -d ./src/shared/infra/typeorm/data-source.ts migration:revert",
        "test:dev": "cross-var set NODE_ENV=test&& jest src/ --runInBand --detectOpenHandles --config=jest.config.ts",
        "test:prod": "cross-var set NODE_ENV=prod&& jest dist/ --runInBand --config=jest.config.js",
        "seed:admin": "set NODE_ENV=dev&& ts-node-dev -r tsconfig-paths/register --poll --transpile-only --inspect --ignore-watch node_modules ./src/shared/infra/typeorm/seed/admin",
        "win-script": "cross-var yarn echo $datasource datasource Hello $npm_package_name %name% typeorm datasource"
    },
    "devDependencies": {
        "@babel/cli": "^7.21.0",
        "@babel/core": "^7.21.3",
        "@babel/node": "^7.20.7",
        "@babel/plugin-proposal-class-properties": "^7.18.6",
        "@babel/plugin-proposal-decorators": "^7.21.0",
        "@babel/preset-env": "^7.20.2",
        "@babel/preset-typescript": "^7.21.0",
        "@types/bcryptjs": "^2.4.2",
        "@types/cors": "^2.8.13",
        "@types/express": "^4.17.17",
        "@types/jest": "^29.5.0",
        "@types/jsonwebtoken": "^9.0.1",
        "@types/mime": "^3.0.1",
        "@types/multer": "^1.4.7",
        "@types/nodemailer": "^6.4.7",
        "@types/redis": "^4.0.11",
        "@types/swagger-ui-express": "^4.1.3",
        "@types/uuid": "^9.0.1",
        "@typescript-eslint/eslint-plugin": "^5.55.0",
        "@typescript-eslint/parser": "^5.55.0",
        "babel-plugin-module-resolver": "^5.0.0",
        "babel-plugin-transform-typescript-metadata": "^0.3.2",
        "eslint": "^8.36.0",
        "eslint-config-airbnb-base": "^15.0.0",
        "eslint-config-prettier": "^8.7.0",
        "eslint-import-resolver-typescript": "^3.5.3",
        "eslint-plugin-import": "^2.27.5",
        "eslint-plugin-import-helpers": "^1.3.1",
        "eslint-plugin-prettier": "^4.2.1",
        "jest": "^29.5.0",
        "prettier": "^2.8.4",
        "ts-node-dev": "^2.0.0",
        "tsconfig-paths": "^4.1.2",
        "typescript": "^4.9.4"
    },
    "dependencies": {
        "@aws-sdk/client-s3": "^3.294.0",
        "@aws-sdk/client-ses": "^3.294.0",
        "@sentry/node": "^7.44.1",
        "@sentry/tracing": "^7.44.1",
        "@types/supertest": "^2.0.12",
        "await-to-js": "^3.0.0",
        "bcryptjs": "^2.4.3",
        "class-transformer": "^0.5.1",
        "cors": "^2.8.5",
        "cross-var": "^1.1.0",
        "csv-parse": "^5.3.6",
        "dayjs": "^1.11.7",
        "dotenv": "^16.0.3",
        "express": "^4.18.2",
        "express-async-errors": "^3.1.1",
        "handlebars": "^4.7.7",
        "jsonwebtoken": "^9.0.0",
        "mime": "^3.0.0",
        "multer": "^1.4.5-lts.1",
        "nodemailer": "^6.9.1",
        "pg": "^8.10.0",
        "rate-limiter-flexible": "^2.4.1",
        "redis": "^4.6.5",
        "reflect-metadata": "^0.1.13",
        "supertest": "^6.3.3",
        "swagger-ui-express": "^4.6.2",
        "ts-jest": "^29.0.5",
        "tsyringe": "^4.7.0",
        "typeorm": "0.3.11",
        "uuid": "^9.0.0"
    }
}
