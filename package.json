{
  "name": "rentalx",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "typeorm": "set NODE_ENV=dev&& ts-node-dev -r tsconfig-paths/register ./node_modules/typeorm/cli",
    "dev": "ts-node-dev -r tsconfig-paths/register --poll --transpile-only --inspect --ignore-watch node_modules --respawn src/shared/infra/http/server.ts",
    "migration:create": "yarn typeorm migration:create src/shared/infra/typeorm/migrations/%name%",
    "migration:generate": "yarn typeorm -d ./src/shared/infra/typeorm/data-source.ts migration:generate ./src/shared/infra/typeorm/migrations/generate/",
    "migration:run": "yarn typeorm -d ./src/shared/infra/typeorm/data-source.ts migration:run",
    "migration:revert": "yarn typeorm -d ./src/shared/infra/typeorm/data-source.ts migration:revert",
    "test": "cross-var set NODE_ENV=test&& jest --runInBand",
    "seed:admin": "set NODE_ENV=dev&& ts-node-dev -r tsconfig-paths/register --poll --transpile-only --inspect --ignore-watch node_modules ./src/shared/infra/typeorm/seed/admin",
    "win-script": "cross-var yarn echo $datasource datasource Hello $npm_package_name %name% typeorm datasource"
  },
  "devDependencies": {
    "@swc/core": "^1.3.38",
    "@swc/jest": "^0.2.24",
    "@types/bcryptjs": "^2.4.2",
    "@types/express": "^4.17.15",
    "@types/jest": "^29.4.0",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/multer": "^1.4.7",
    "@types/nodemailer": "^6.4.7",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^5.48.0",
    "@typescript-eslint/parser": "^5.48.0",
    "eslint": "^7.21.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-import-helpers": "^1.3.1",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.4.3",
    "prettier": "^2.8.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.1.2",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "@types/supertest": "^2.0.12",
    "await-to-js": "^3.0.0",
    "bcryptjs": "^2.4.3",
    "cross-var": "^1.1.0",
    "csv-parse": "^5.3.3",
    "dayjs": "^1.11.7",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "handlebars": "^4.7.7",
    "jsonwebtoken": "^9.0.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.1",
    "pg": "^8.9.0",
    "reflect-metadata": "^0.1.13",
    "supertest": "^6.3.3",
    "swagger-ui-express": "^4.6.0",
    "ts-jest": "^29.0.5",
    "tsyringe": "^4.7.0",
    "typeorm": "^0.3.11",
    "uuid": "^9.0.0"
  }
}
